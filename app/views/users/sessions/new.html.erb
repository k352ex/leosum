<section class="row">
  <section class="col-md-12">
  <div class="Main_body">
    <div class="header">

      <%= image_tag "logo.png" , width: 750, height: 400 , style: "text-align:left; margin-bottom: 100px"%>

      <div class="form" id="main_form">
        <%= form_for(resource, as: resource_name, url: session_path(resource_name))do |f| %>
          <div class="form-group" id="form_group">
            <%= f.email_field :email, class: "form-control", id: "exampleInputEmail1", placeholder: "이메일", autofocus: true, :required => true %>
          </div>

          <div class="form-group" id="form_group">
            <%= f.password_field :password, class: "form-control", id: "exampleInputPassword1", placeholder: "비밀번호", autocomplete: "off" , :required => true%>
          </div>
          <div class="section_login_bar">
              <%= link_to "회원 가입", new_user_registration_path , class: "btn btn-theme-dark btn-md" , id: "link_setting" %>
              <%= f.submit "로그인", class: "btn btn-theme-dark" , id: "login_setting"%>
          </div>
        <% end %>
      </div>
    </div>
  </div>
  </section>
</section>
